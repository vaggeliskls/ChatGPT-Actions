{
  "name": "chat-gpt",
  "version": "1.0.0",
  "description": "Chat gpt usage",
  "main": "index.js",
  "bin": "dist/index.js",
  "exports": "./dist/index.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": "^20.10.0",
  "type": "modeule"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf dist/",
    "build": "npm run clean && tsc",
    "build:ncc": "npm run clean && ncc build ./src/index.ts --target es2020 -a -m -o dist",
    "start:prod": "node dist/index.js",
    "start": "npm run --silent build && node dist/index.js",
    "lint": "eslint .",
    "lint:ci": "eslint . --cache --cache-location ./node_modules/.cache/eslint/.eslintcache",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "format:check:ci": "prettier --check --cache .",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "chatgpt": "^5.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@swc/core": "^1.3.100",
    "@swc/helpers": "^0.5.3",
    "@swc/jest": "^0.2.29",
    "@types/decompress": "^4.2.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/listr": "^0.14.7",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.8.10",
    "@types/nunjucks": "^3.2.5",
    "@types/randomstring": "^1.1.10",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vercel/ncc": "^0.38.1",
    "@yao-pkg/pkg": "^5.11.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-cucumber": "^3.0.1",
    "lint-staged": "^15.2.0",
    "onchange": "^7.1.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "run-script-os": "^1.1.6",
    "swc-loader": "^0.2.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --cache-location ./node_modules/.cache/eslint/.eslintcache",
    "*": "prettier --list-different --ignore-unknown"
  }
}
